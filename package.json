  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CodesmithLLC/testing-module/issues"
  },
  "homepage": "https://github.com/CodesmithLLC/testing-module#readme",
  "dependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "blanket": "^1.2.1",
    "body-parser": "^1.14.2",
    "chai": "^3.5.0",
    "expect": "^1.14.0",
    "express": "^4.13.4",
    "harp": "^0.20.1",
    "istanbul": "^0.4.2",
    "jquery": "^2.2.0",
    "mocha": "^2.4.5",
    "mocha-phantomjs": "^4.0.2",
    "phantomjs": "^2.1.3",
    "zombie": "^4.2.1"
