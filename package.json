{
  "name": "testing-module",
  "version": "1.0.0",
  "description": "Codesmith testing module",
  "scripts": {
    "start": "nodemon index.js",
    "test": "npm run test-front && mocha --timeout 8000 --recursive test/ -R nyan",
    "coverage": "istanbul cover _mocha -- -R spec",
    "build-html": "harp compile public test/compiled",
    "test-front": "mocha-phantomjs test/compiled/index.html"
  },
