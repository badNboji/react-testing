{
  "name": "testing-module",
  "version": "1.0.0",
  "description": "Codesmith testing module",
  "scripts": {
    "start": "nodemon index.js",
    "test": "npm run phantom; mocha --timeout 8000 test/",
    "build-html": "harp compile public test/compiled",
    "babeljs": "babel public/js/main.js --out-file public/build/main.js --presets es2015",
    "phantom": "npm run babeljs && mocha-phantomjs test/compiled/index.html",
    "supertest": "mocha --timeout 8000 test/supertest.js",
    "zombie": "mocha --timeout 8000 test/zombie.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodesmithLLC/testing-module.git"
  },
  "author": "David Petri <petridw@gmail.com> (davidpetri.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CodesmithLLC/testing-module/issues"
  },
  "homepage": "https://github.com/CodesmithLLC/testing-module#readme",
  "dependencies": {
    "body-parser": "^1.14.2",
    "express": "^4.13.4",
    "harp": "^0.20.1",
    "solve-boggle": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "blanket": "^1.2.1",
    "browserify": "^13.0.0",
    "chai": "^3.5.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.2.3",
    "expect": "^1.14.0",
    "mocha": "^2.4.5",
    "mocha-phantomjs": "^4.0.2",
    "nodemon": "^1.8.1",
    "phantomjs": "^2.1.3",
    "supertest": "^1.1.0",
    "zombie": "^4.2.1"
  }
}
